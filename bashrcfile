alias kill_ros=killall_ros_func 
alias kill_gazebo=killall_gazebo_func 
 
function killall_ros_func() 
{ 
            kill -9 $(ps aux | grep ros | awk '{print $2}') 
} 
 
function killall_gazebo_func() 
    { 
            kill -9 $(ps aux | grep gazebo | awk '{print $2}'); 
            kill -9 $(ps aux | grep gz | awk '{print $2}') 
    }

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/guanrui/anaconda2/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/guanrui/anaconda2/etc/profile.d/conda.sh" ]; then
        . "/home/guanrui/anaconda2/etc/profile.d/conda.sh"
    else
        export PATH="/home/guanrui/anaconda2/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

